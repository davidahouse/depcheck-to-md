{
  "name": "depcheck-to-md",
  "main": "bin/depcheck-to-md.js",
  "version": "1.0.0",
  "description": "A command line app for turning depcheck output to markdown",
  "license": "MIT",
  "repository": "davidahouse/depcheck-to-md",
  "preferGlobal": true,
  "bin": {
    "depcheck-to-md": "bin/depcheck-to-md.js"
  },
  "dependencies": {
    "glob": "^7.1.6"
  },
  "devDependencies": {
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0"
  },
  "scripts": {
    "lint": "eslint $(find . -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./public/*\")",
    "lint-fix": "eslint --fix $(find . -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./public/*\")"
  }
}
